%header.header-fixed
  .container-fluid
    .row
      .col-xs-6
        %h1= model_name.pluralize
      .col-xs-6
        = link_to icon("plus", "New #{model_name}"), url_for([:new, model_sym]), class: "btn btn-success pull-right"

.container-fluid
  = render 'table'
  
  = paginate @collection, theme: 'twitter-bootstrap-3'